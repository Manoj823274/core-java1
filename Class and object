code 2:
class Book {
    String title;
    String author;
 Book(String t, String a) {
        title = t;
        author = a;
    }
 void show() {
        System.out.println("Title: " + title + ", Author: " + author);
    }
}
public class Main {
    public static void main(String[] args) {
        Book b1 = new Book("Java Basics", "James Gosling");
        Book b2 = new Book("Spring Boot", "Rod Johnson");

        b1.show();
        b2.show();
    }
}
-----------------------------------------------------------
code 3:
class Calculator {
    int add(int a, int b) {
        return a + b;
    }
int multiply(int x, int y) {
        return x * y;
    }
}
public class Main {
    public static void main(String[] args) {
        Calculator calc = new Calculator();
        System.out.println("Sum: " + calc.add(5, 10));
        System.out.println("Product: " + calc.multiply(4, 6));
    }
}
